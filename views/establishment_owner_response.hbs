    <!-- POPUP SECTION-->
    <div class="popup-review-comment-container">
        <div class="popup-content">
            <div class="container-top">
                <button class="popup-close-button" onclick="hidePopup()">
                    Close
                </button>
            </div>
            <div class="container-bottom"></div>
        </div>
    </div>
</div>
<script>
    let subMenu = document.getElementById("subMenu");
    function toggleMenu() {
    subMenu.classList.toggle("open-menu");
    }
</script>